{
    "name": "funtimecoding/php-skeleton",
    "description": "skeleton for PHP projects to simplify Jenkins integration",
    "license": "proprietary",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*",
        "sebastian/phpcpd": "2.*",
        "pdepend/pdepend": "2.*",
        "phploc/phploc": "2.*",
        "covex-nn/phpcb": "1.*",
        "phpmd/phpmd": "2.*",
        "theseer/phpdox": "0.*",
        "behat/behat": "3.*",
        "phpspec/phpspec": "2.*",
        "filp/whoops": "1.*"
    }
}
